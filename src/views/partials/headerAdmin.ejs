<header class="admin__header">
    <div class="admin__logo">
        <a class="admin__logo--a" href="/"><h1>FreeLab</h1></a>
    </div>
    <div class="admin__busqueda">
        <form class="admin__formulario-busqueda" action="/resultado" method="GET">
          <input type="text" name="key" placeholder="Buscar..." class="admin__entrada-busqueda">
          <button type="submit" class="admin__boton-busqueda">Buscar</button>
        </form>
    </div>
    <div class="admin__filtro">
        <label class="admin__etiqueta-filtro">Filtrar por:</label>
        <select class="admin__select-filtro">
            <option value="todos" selected>Todos</option>
            <option value="contratados">Contratados</option>
            <option value="contratantes">Contratantes</option>
        </select>
    </div>
    <a href="/productos/formularioProducto" class="boton__primario">Crear nuevo</a>
</header>