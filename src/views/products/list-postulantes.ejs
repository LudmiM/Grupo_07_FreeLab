<!DOCTYPE html>
<html lang="es">
<%-include('../partials/head')%>
<body>
    <%-include('../partials/header', {title: 'Postulantes | FreeLab' })%>
    <main class="card edit-product-card">
      <!--Muestr el titulo del proyecto-->
      <!--Muestr el titulo de perfil requerido del freelancer-->
      <!--Muestr el los freelancers postulados-->
      <section class="individuals__container">
        <% Postulantes.forEach(p => { %>
          <article class="article__individuals">
            <p><%= p.specialty %></p>
            <!--AddLink-->
            <p><%= p.about %></p>
            <p><%= p.price %></p>
            <article class="section__explicacion__article__habilidades article__skills">
                
            </article>
          </article>   
        <% }) %>
      </section>
    </main>
    <%-include('../partials/footer')%>
</body>
<%-include('../partials/scripts')%>    
</html>